{
  "manifest_version": 2,

  "name": "Cipher",
  "description": "This extension help users encrypt messages",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Cipher"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}